target-y := default

ifneq ($(filter msc,$(CONFIG_COMPILER)),)
ifneq ($(filter x64,$(CONFIG_COMPILER)),)
default-y := src/lang/builtin/ src/lang/coro/msc_x64/ config/coro_test/
else
default-y := src/lang/builtin/ src/lang/coro/msc_x86/ config/coro_test/
endif
else
ifneq ($(filter x64,$(CONFIG_COMPILER)),)
default-y := src/lang/builtin/ src/lang/coro/gnu_x64/ config/coro_test/
else
default-y := src/lang/builtin/ src/lang/coro/gnu_x86/ config/coro_test/
endif
endif

default-binary-type := exe
